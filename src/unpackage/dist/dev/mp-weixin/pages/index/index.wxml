<view class="page" bind:__l="__l"><cu-custom bgColor="bg-gradual-pink" isBack="{{true}}" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">每日美图</view></cu-custom><scroll-view scroll-y><view class="cu-card case"><block wx:for="{{images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item shadow" data-id="{{index}}" data-url="{{item.url}}"><view class="image"><image src="{{'https://cn.bing.com'+item.url}}" mode="widthFix"></image><view class="cu-bar bg-shadeBottom"><text class="text-cut">{{item.desc}}</text></view></view><view class="cu-list list-avatar"><view class="cu-item"><view class="content flex-sub"><view class="text-sm flex justify-between"><view class="text-sm"><text class="margin-lr-xs"></text>{{''+item.copyright+''}}</view></view></view></view></view></view></block></view></scroll-view></view>